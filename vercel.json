{
  "rewrites": [
    {
      "source": "/api/admin/security",
      "destination": "/api/security"
    },
    {
      "source": "/api/admin/login",
      "destination": "/api/login"
    },
    {
      "source": "/api/admin/password",
      "destination": "/api/password"
    },
    {
      "source": "/api/admin/logout",
      "destination": "/api/logout"
    },
    {
      "source": "/api/site-config",
      "destination": "/api/site-config"
    },
    {
      "source": "/api/admin/site-config",
      "destination": "/api/admin-site-config"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
